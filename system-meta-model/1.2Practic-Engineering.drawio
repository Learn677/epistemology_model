        <mxCell id="Jqv_VF9yep6gmFjdm9qP-1" value="&lt;blockquote style=&quot;margin: 0 0 0 40px; border: none; padding: 0px;&quot;&gt;&lt;b&gt;Блок 1: Принципы реализации модели (С помощью каких инструментов реализована модель?)&lt;/b&gt;&lt;/blockquote&gt;&lt;blockquote style=&quot;margin: 0 0 0 40px; border: none; padding: 0px;&quot;&gt;&lt;div&gt;&lt;/div&gt;&lt;/blockquote&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;fontFamily=Verdana;strokeWidth=10;fontSize=48;" vertex="1" parent="1">
          <mxGeometry x="7848" y="-1548" width="1584" height="504" as="geometry" />
        </mxCell>
        <mxCell id="K1LrehUICKeunIkjDZvf-4" value="&lt;blockquote style=&quot;margin: 0 0 0 40px; border: none; padding: 0px;&quot;&gt;&lt;font&gt;&lt;b&gt;6 уровень - ПРИНЦИПЫ&lt;/b&gt;&lt;/font&gt;&lt;/blockquote&gt;" style="rounded=0;whiteSpace=wrap;fontFamily=Verdana;align=left;strokeWidth=10;html=1;fontSize=48;" vertex="1" parent="1">
