        <mxCell id="1_kr48qRO-vhiNAYgfr9-2" value="&lt;font style=&quot;font-size: 24px;&quot;&gt;&lt;b style=&quot;&quot;&gt;Методы Aggregate,Count, Sum, MIn, Max, Avarage&lt;/b&gt;&lt;/font&gt;" style="rounded=0;whiteSpace=wrap;html=1;" parent="1" vertex="1">
