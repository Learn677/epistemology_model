t;&quot; class=&quot;token punctuation&quot;&gt;(&lt;/span&gt;sortedArray&lt;span style=&quot;&quot; class=&quot;token punctuation&quot;&gt;[&lt;/span&gt;mid&lt;span style=&quot;&quot; class=&quot;token punctuation&quot;&gt;]&lt;/span&gt; &lt;span style=&quot;&quot; class=&quot;token operator&quot;&gt;&amp;lt;&lt;/span&gt; target&lt;span style=&quot;&quot; class=&quot;token punctuation&quot;&gt;)&lt;/span&gt;&lt;br&gt;            left &lt;span style=&quot;&quot; class=&quot;token operator&quot;&gt;=&lt;/span&gt; mid &lt;span style=&quot;&quot; class=&quot;token operator&quot;&gt;+&lt;/span&gt; &lt;span style=&quot;&quot; class=&quot;token number&quot;&gt;1&lt;/span&gt;&lt;span style=&quot;&quot; class=&quot;token punctuation&quot;&gt;;&lt;/span&gt; &lt;span style=&quot;&quot; class=&quot;token comment&quot;&gt;// Ищем в правой половине&lt;/span&gt;&lt;br&gt;        &lt;span style=&quot;&quot; class=&quot;token keyword&quot;&gt;else&lt;/span&gt;&lt;br&gt;            right &lt;span style=&quot;&quot; class=&quot;token operator&quot;&gt;=&lt;/span&gt; mid &lt;span style=&quot;&quot; class=&quot;token operator&quot;&gt;-&lt;/span&gt; &lt;span style=&quot;&quot; class=&quot;token number&quot;&gt;1&lt;/span&gt;&lt;span style=&quot;&quot; class=&quot;token punctuation&quot;&gt;;&lt;/span&gt; &lt;span style=&quot;&quot; class=&quot;token comment&quot;&gt;// Ищем в левой половине&lt;/span&gt;&lt;br&gt;    &lt;span style=&quot;&quot; class=&quot;token punctuation&quot;&gt;}&lt;/span&gt;&lt;br&gt;&lt;br&gt;    &lt;span style=&quot;&quot; class=&quot;token keyword&quot;&gt;return&lt;/span&gt; &lt;span style=&quot;&quot; class=&quot;token operator&quot;&gt;-&lt;/span&gt;&lt;span style=&quot;&quot; class=&quot;token number&quot;&gt;1&lt;/span&gt;&lt;span style=&quot;&quot; class=&quot;token punctuation&quot;&gt;;&lt;/span&gt; &lt;span style=&quot;&quot; class=&quot;token comment&quot;&gt;// Элемент не найден&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;&quot; class=&quot;token punctuation&quot;&gt;}&lt;/span&gt;&lt;/font&gt;&lt;/pre&gt;&lt;pre style=&quot;font-family: var(--ds-font-family-code); overflow: auto; padding: calc(var(--ds-md-zoom)*8px)calc(var(--ds-md-zoom)*12px); text-wrap-mode: wrap; word-break: break-all; margin-top: 0px !important; margin-bottom: 0px !important;&quot;&gt;&lt;font style=&quot;color: rgb(0, 0, 0);&quot;&gt;&lt;span style=&quot;&quot; class=&quot;token punctuation&quot;&gt;&lt;br&gt;&lt;/span&gt;&lt;/font&gt;&lt;/pre&gt;&lt;pre style=&quot;overflow: auto; padding: calc(var(--ds-md-zoom)*8px)calc(var(--ds-md-zoom)*12px); word-break: break-all; margin-top: 0px !important; margin-bottom: 0px !important;&quot;&gt;&lt;font style=&quot;color: rgb(0, 0, 0);&quot;&gt;&lt;span style=&quot;&quot; class=&quot;token punctuation&quot;&gt;&lt;div style=&quot;color: rgb(0, 0, 0); font-family: var(--ds-font-family-code); white-space: normal;&quot;&gt;&lt;div style=&quot;font-family: Helvetica;&quot;&gt;&lt;b&gt;------------------------------------------------------------------------------&lt;/b&gt;&lt;b style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;-----------------------------------------------------------------------------------------------------------------------------------&lt;/b&gt;&lt;/div&gt;&lt;div&gt;&lt;b style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;&lt;br&gt;&lt;/b&gt;&lt;/div&gt;&lt;/div&gt;&lt;div style=&quot;color: rgb(0, 0, 0); text-wrap-mode: wrap;&quot;&gt;&lt;b style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;&lt;font face=&quot;Helvetica&quot;&gt;Экспоненциальная сложность O(2^n) - два вызова степень - значение данных&lt;/font&gt;&lt;/b&gt;&lt;/div&gt;&lt;div style=&quot;color: rgb(0, 0, 0); font-family: var(--ds-font-family-code); text-wrap-mode: wrap;&quot;&gt;&lt;b style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;&lt;br&gt;&lt;/b&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&lt;font face=&quot;Helvetica&quot;&gt;&lt;span style=&quot;text-wrap-mode: wrap;&quot;&gt;static int Fibonacci(int n)&lt;br&gt;{&lt;br&gt;    if (n &amp;lt;= 1)&lt;br&gt;        return n;&lt;br&gt;    return Fibonacci(n - 1) + Fibonacci(n - 2);&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;// каждый вызов пораждает два новых&lt;br&gt;}&lt;/span&gt;&lt;/font&gt;&lt;b style=&quot;color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255)); font-family: var(--ds-font-family-code); text-wrap-mode: wrap;&quot;&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&lt;font face=&quot;Helvetica&quot;&gt;&lt;span style=&quot;text-wrap-mode: wrap;&quot;&gt;&lt;br&gt;&lt;/span&gt;&lt;/font&gt;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&lt;font face=&quot;Helvetica&quot;&gt;&lt;span style=&quot;text-wrap-mode: wrap;&quot;&gt;&lt;b&gt;Та же реализация, только со сложностью O(n)&lt;/b&gt;&lt;/span&gt;&lt;/font&gt;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&lt;font face=&quot;Helvetica&quot;&gt;&lt;span style=&quot;text-wrap-mode: wrap;&quot;&gt;&lt;br&gt;&lt;/span&gt;&lt;/font&gt;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&lt;font face=&quot;Helvetica&quot;&gt;&lt;span style=&quot;text-wrap-mode: wrap;&quot;&gt;static long Fibonacci(int n)&lt;br&gt;{&lt;br&gt;    if (n &amp;lt;= 1)&lt;br&gt;        return n;&lt;/span&gt;&lt;/font&gt;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&lt;font face=&quot;Helvetica&quot;&gt;&lt;span style=&quot;text-wrap-mode: wrap;&quot;&gt;&lt;br&gt;    long a = 0;  // F(0) - исходное значение&lt;br&gt;    long b = 1;  // F(1) - будет результатом последующих итераций&lt;br&gt;    long temp;&lt;br&gt;    &lt;br&gt;    for (int i = 2; i &amp;lt;= n; i++) - счётчик = 2, т.к. пара 0 и 1 уже обработаны ранее (заданы по умолчанию)&lt;br&gt;    {&lt;br&gt;        temp = a + b;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;// temp - записывает сумму чисел&lt;br&gt;        a = b;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;// a=b перемещение предыдующего результата в А&lt;br&gt;        b = temp;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;// b - результат суммы на одном обходе цикла&lt;br&gt;    }&lt;br&gt;    &lt;br&gt;    return b;&lt;br&gt;}&lt;/span&gt;&lt;/font&gt;&lt;/span&gt;&lt;/div&gt;&lt;/span&gt;&lt;/font&gt;&lt;/pre&gt;&lt;/div&gt;&lt;/blockquote&gt;" style="rounded=0;whiteSpace=wrap;html=1;" parent="1" vertex="1">
